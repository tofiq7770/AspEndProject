
@model FeatureDetailVM

<div class="container mt-5 mx-3">

    <h3> Name: @Model.Name </h3>
    <h3> Description: @Model.Description </h3>
    <h3> Icon: @Model.Icon </h3>
    
    
    <div class=mt-4>

        <a class="btn btn-primary" asp-area="Admin" asp-controller="Feature" asp-action="Index">Back</a>
        <a class="btn btn-primary" asp-area="Admin" asp-controller="Feature" asp-route-id="@Model.Id" asp-action="Update">Update</a>
        <a class="btn btn-danger" asp-area="Admin" asp-controller="Feature" asp-route-id="@Model.Id" asp-action="Delete">Delete</a>
    </div>
</div>