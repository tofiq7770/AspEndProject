
@model FactContentDetailVM

<div class="container mt-5 mx-3">

    <h3> Title: @Model.Title </h3>
    <h3> NumberInfo: @Model.NumberInfo </h3>
    <h3> Icon: @Model.Icon </h3>
    
    
    <div class=mt-4>

        <a class="btn btn-primary" asp-area="Admin" asp-controller="FactContent" asp-action="Index">Back</a>
        <a class="btn btn-primary" asp-area="Admin" asp-controller="FactContent" asp-route-id="@Model.Id" asp-action="Update">Update</a>
        <a class="btn btn-danger" asp-area="Admin" asp-controller="FactContent" asp-route-id="@Model.Id" asp-action="Delete">Delete</a>
    </div>
</div>